package com.ust.iocautowiring;

public class B {
	A a;
	
	
	/*
	 * public B(A a) { this.a=a; System.out.println("constructor"); }
	 */
	
	
	public void setA(A a) {
		//by name and bytype will use setter injection
		System.out.println("setA() called");
		this.a=a;
	}


	public B() {
		super();
	}

}
